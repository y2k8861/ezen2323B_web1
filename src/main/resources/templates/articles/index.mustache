{{>layouts/header}}  <!-- 헤더 파일 호출 -->
<table border="1">
    <thead>
        <tr>
            <th>ID</th>
            <th>Title</th>
            <th>Content</th>
        </tr>
    </thead>

    <tbody>
        {{#articleList}}
        <tr>
            <td>{{id}}</td>
            <td><a href="/articles/{{id}}">{{title}}</a></td>
            <td>{{content}}</td>
        </tr>
        {{/articleList}}
    </tbody>
</table>

<a href="/articles/new">글쓰기(New article) </a>
{{>layouts/footer}}  <!-- 푸터 파일 호출 -->